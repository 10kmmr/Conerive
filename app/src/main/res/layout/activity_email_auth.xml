<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.sage.mapsexample.MainActivity"
    android:background="@drawable/front_page_background">


    <EditText
        android:id="@+id/EmailE"
        android:layout_width="252dp"
        android:layout_height="wrap_content"
        android:layout_above="@+id/PasswordE"
        android:layout_alignLeft="@+id/PasswordE"
        android:layout_alignStart="@+id/PasswordE"
        android:layout_marginBottom="73dp"
        android:ems="10"
        android:hint="Email"
        android:inputType="textPersonName"
        android:text="hvprithvi09@gmail.com"
        android:visibility="visible" />

    <EditText
        android:id="@+id/PasswordE"
        android:layout_width="252dp"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        android:ems="10"
        android:hint="Password"
        android:inputType="textPassword"
        android:text="pageupto123"
        android:visibility="visible" />

    <Button
        android:id="@+id/signUp"
        android:layout_width="252dp"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/PasswordE"
        android:layout_alignParentBottom="true"
        android:layout_alignStart="@+id/PasswordE"
        android:layout_marginBottom="65dp"
        android:layout_marginLeft="14dp"
        android:layout_marginStart="14dp"
        android:background="@drawable/rounded_button"
        android:text="Signup"
        android:textColor="@drawable/button_text"
        android:visibility="visible" />

    <Button
        android:id="@+id/loginE"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/signUp"
        android:layout_alignStart="@+id/signUp"
        android:layout_below="@+id/PasswordE"
        android:layout_marginLeft="35dp"
        android:layout_marginStart="35dp"
        android:layout_marginTop="47dp"
        android:text="login" />

    <!--<com.facebook.login.widget.LoginButton-->
